<?xml version="1.0" encoding="UTF-8"?>
<plugin title="edut" loader="DEFAULT">
<overlays>
		<overlay id="startup.edut" resourcebundle="content/edut/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="edut" label="${Launcher.edut}" command="mantleXulHandler.openUrl('${Launcher.edut}','${Launcher.edut_TOOLTIP}','plugin/edut/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/edut/static" localFolder="static"/>
		<static-path url="/edut/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>Edut</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="edut"
		title="Edut"
		type="edut"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="edut_main" anchor="tools-submenu" label="Edut" command="content/edut/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
